- breadcrumb :logouts
= render partial: 'layouts/breadcrumbs'

.mypage-main
  .logout-content
    =form_with url: destroy_user_session_path, local: true, method: :delete, class: "logout-content__form" do |f|
      =f.submit "ログアウト", class: "logout-content__form--btn"
  =render partial: "users/mypage_side_bar", locals: { user: @user}
